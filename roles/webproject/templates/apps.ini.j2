[uwsgi]
protocol = http
module = wsgi:app
master = true
processes = 5
http-socket = 127.0.0.1:5555
socket = 127.0.0.1:5555
uid = 33
gid = 33
chmod-socket = 664
vacuum = true
die-on-term = true
wsgi-file = {{ app_file }}
callable = app
chdir = {{ project_dir }}
logto = /var/log/uwsgi/%n.log
env = FLASK_LOG_TO_DIR={{ project_dir }}/logs 
env = FLASK_LOG_LEVEL=DEBUG
env = FLASK_CALCULATE_WAIT_TIME=2
env = FLASK_BID_CREATION_WAIT_TIME=2