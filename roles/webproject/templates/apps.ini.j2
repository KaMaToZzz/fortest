[uwsgi]
protocol = http
module = wsgi:app
master = true
processes = 5
http-socket = :5555
socket = app.sock
chmod-socket = 660
vacuum = true
die-on-term = true
wsgi-file = {{ app_file }}
callable = app
chdir = {{ project_dir }}
logto = /var/log/uwsgi/%n.log
env = FLASK_LOG_TO_DIR={{ project_dir }}/logs 
env = FLASK_LOG_LEVEL=DEBUG
env = FLASK_CALCULATE_WAIT_TIME=2
env = FLASK_BID_CREATION_WAIT_TIME=2